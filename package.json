{
  "name": "habit-tracker-api",
  "version": "0.1.0",
  "description": "Habit Tracker API - Track your habits and consistency with data-driven insights",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "postinstall": "prisma generate",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "habit-tracker",
    "fastify",
    "prisma",
    "typescript",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@prisma/client": "^6.0.0",
    "@sinclair/typebox": "^0.34.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.4.0",
    "fastify": "^5.0.0",
    "moment": "^2.30.1",
    "pino": "^8.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prisma": "^6.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
